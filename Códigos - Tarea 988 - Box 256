1 ) Big square
PIX @18 002 000
ADD @16 @18 @18
MOV @18 @20 001
MOV @19 @18 008
FLP @16 @17 001
JMP @00 001 010
011 031 021 032
0D3 01D 0E3 01E


2) Big square II
PIX @18 002 000
ADD @16 @18 @18
MOV @18 @20 001
MOV @19 @18 008
FLP @16 @17 001
JMP @00 001 010
011 031 021 032
0D3 01D 0E3 01E


3) Checkerboard 
ADD 001 @11 @11
MUL 088 @11 @12
DIV @12 080 @12
ADD 002 @12 @12
PIX 0FF 000 000

4) Checkerboard II
ADD *00 001 *00
DIV *00 002 @2F
MUL 088 @2F @2F
PIX *00 001 000
JGR 080 @2F @00
PIX *00 00C 000



5) Four squares
    
PIX 013 008 000
ADD @14 @01 @01
ADD @05 001 @05
JGR 020 @05 @00
MOV 014 @05 001
SUB @02 0FF @02
0CD 023 0FD 0F0
0F3 0FF 0FF 056

    
    
6) Four squares II
  
  
PIX @03 008 000
ADD 001 @03 @03
JGR @40 @03 @00
ADD 010 @3F @3F
ADD 010 @40 @40
MOV @3F @03 000
ADD 001 @41 @41
JGR @3E @41 @00
ADD 001 @02 @02
ADD @3E @03 @03
DIV @3E 002 @3E
MOV @03 @3F 000
ADD @3E @3F @40
MOV 000 @41 000
JNE 00C @02 @00
JMP 000 008 000
008 000 000 000


7) Carpet

PIX 000 *1A 000
ADD 001 @01 @01
ADD 001 @1A @1A
MOD @01 010 @03
JNE @03 000 @00
SUB @1A 00F @1A
JMP @00 01B 002
001 003 004 001
001 004 003 001
004 001 001 004
001 001 002 001
001 004 001 003
004 001 001 004
003 001 004 001
001 002 000 000

8) Carpet II
ADD 001 @0D @0D
FLP @0E @0F 001
MOD @0D 005 @17
PIX 0FF 00B 003
JNE @17 002 @00
PIX @0D @0F 000

9) Sierpinski
PIX 000 @13 000
ADD @13 @12 @23
ADD 001 @01 @01
MOV @13 @12 012
JMP @00 000 008

10) Sierpinski II
ADD @23 @22 @33
ADD 001 @1D @1D
MOV @23 @22 012
JEQ 000 @22 @00
MOD @22 002 @1E
JEQ 001 @1E @1C
MOV 00C @1E 001
PIX 0FF 000 000
JMP @00 000 001


11) Smiley Face
PIX 0A1 00A 000
SUB @1A 010 @1A
ADD @01 001 @01
JGR @1A 00F @00
ADD @1A @01 @01
MOV @1B @1A 020
JMP @00 021 081
022 031 061 033
036 035 0A7 08A
04C 04A 087 0A5
041 021 043 051
021 052 051 021
051 061 021 000

12) Mario
  MOV @1A @06 001
  PIX 005 000 000
  SUB @06 010 @06
  ADD 001 @05 @05
  JGR @06 00F @04
  ADD 001 @01 @01
  JMP @00 058 0A0
  098 070 034 02F
  014 01F 080 014
  01F 014 03F 014
  03F 060 014 01F
  024 03F 014 03F
  060 014 04F 044
